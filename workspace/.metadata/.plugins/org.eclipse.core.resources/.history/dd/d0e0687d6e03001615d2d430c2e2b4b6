
public class Funcionario {
	
	String nome;
	Double salario;
	Character idade;	
	
	
	public String getNome() {
		return nome;
	}
	public Double getSalario() {
		return salario;
	}
	public Character getIdade() {
		return idade;
	}
	public void setIdade(Character idade) {
		this.idade = idade;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
	public void setSalario(Double salario) {
		this.salario = salario;
	}
	

}
